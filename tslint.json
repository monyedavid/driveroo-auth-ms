{
    "defaultSeverity": "error",
    "extends": ["tslint:latest", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "member-access": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "interface-name": false,
        "no-submodule-imports": false,
        "no-return-await": false,
        "no-empty": false,
        "curly": false
        // "indent": [true, "tabs", 4]
    },
    "rulesDirectory": []
}
